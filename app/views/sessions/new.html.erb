<h1>Welcome to Eventify</h1>
<h3>Login</h1>
<%= form_with url: user_sessions_path, local: true do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.submit "Login" %>
<% end %>
<p>
  <a href="<%= new_user_registration_path %>">Create a new account</a>
</p>